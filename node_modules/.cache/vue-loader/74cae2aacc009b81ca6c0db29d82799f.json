etch(`${this.url_base}weather?q=${this.query}&units=metric&APPID=${this.api_key}`)\n         .then(res => {\n           return res.json();\n         }).then(this.setResults);\n         \n      }\n    },\n    setResults (results) {\n      this.weather = results;\n      },\n    dateBuilder(){\n      let d = new Date();\n      let months = [\"January\", \"February\", \"March\", \"April\", \"May\", \"June\", \"July\", \"August\", \"September\", \"October\", \"November\", \"December\"];\n      let days = [\"Sunday\", \"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\", \"Saturday\"];\n      let day = days[d.getDay()];\n      let date = d.getDate();\n      let month = months[d.getMonth()];\n      let year = d.getFullYear();\n      return `${day} ${date} ${month} ${year}`;\n      }\n  }\n}\n\n</script>\n\n<style>\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\nhtml {\n    width: 100%;\n    height: 100%;\n}\n\na {\n    text-decoration: none;\n}\n\nbody{\n  background-image: url('./assets/Paysage-vectoriel-Saint-Malo.png');\n  background-position: bottom;\n  background-repeat: no-repeat;\n}\n\n.search-box{\n  margin-top: 50px;\n  margin-right: 15%;\n  margin-left: 15%;\n}\n\n.search-bar{\nborder-radius: 20px;\nborder: none;\nheight: 50px;\n}\n\n.location{\n  text-align: center;\n  margin-top: 30px;\n  font-size: 50px;\n  color: white;\n  font-weight: 800;\n  font-family: Arial;\n}\n\n.date{\n  text-align: center;\n  margin-top: 23px;\n  font-size: 25px;\n  color: white;\n  font-weight: 800;\n  font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif;\n  opacity: 60%;\n}\n\n.weather-box{\n  margin-top:6vh;\n  margin-right: 30%;\n  margin-left: 30%;\n}\n\n.weather{\n  text-align: center;\n  margin-top: 23px;\n  font-size: 30px;\n  color: #44585a;\n  font-weight: 800;\n  font-family: Arial;\n  background-color: rgb(241, 218, 234);\n  padding: 10px 10px;\n  border-radius: 30px;\n  opacity: 90%;\n}\n\n.temp{\n  width: 40vw;\n  height: 20vw;\n  text-align: center;\n  vertical-align: center;\n  margin-top: 19vh;\n  font-size: 3rem;\n  color: white;\n  font-weight: 800;\n  font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif;\n  background-color: rgb(173, 212, 199);\n  padding: 10px 10px;\n  border-radius: 30px;\n}\n\n@media screen and (min-width: 800px) {\n.search-box{\n  margin-top: 15vh;\n  margin-right: 45%;\n  margin-left: 45%;\n}\n\n.weather-box{\n  margin-right: 45%;\n  margin-left: 45%;\n}\n\n.weather{\n  width: 10vw;\n  height: 4vw;\n}\n\n.temp{\n  width: 10vw;\n  height: 4vw;\n}\n}\n\n@media screen and (min-width: 500px) {\n.search-box{\n  margin-top: 20vh;\n  margin-right: 35%;\n  margin-left: 35%;\n}\n\n.weather-box{\n  margin-right: 40%;\n  margin-left: 40%;\n}\n\n.weather{\n  width: 20vw;\n  height: 10vw;\n}\n\n.temp{\n  width: 20vw;\n  height: 10vw;\n}\n}\n</style>\n"]}]}